s=0
for x in range(1,100):
    s=(s+x)
    print(s, end=" ")

s=0
for x in range(1,100):
    s=(s+x)    
    if(s>1000):
        break
    print(s, end=" ")
        
 a=8989
b=5555
for x in range(100):
    r=a%b
    print(a,"=",b,"*",a//b,"+",r)
    if(r==0):
        break
    a=b
    b=r
print("gcd:",b)

n=18
for x in range(1,19):
    if(n%x==0):
        print(x, end=" ")
    else:
        print("*", end=" ")

n=18
for x in range(1,19):
    if(n%x==0):
        print(x, end=" ")

n=18
count=0
for x in range(1,19):
    if(n%x==0):
        count=count+1
print(count, end=" ")

for n in range(1,101):
    count=0
    for x in range(1,101):
        if(n%x==0):
            count=count+1
    print(count, end=" ")
    
                   

