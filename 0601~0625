w=20000000
b=10000*w/h**2
if(b<=18.5):
    if(18.5<b<=25.0):
        print("classB",end=" ")
    else:
        print("classX",end=" ")
else:
    if(25.0<b<=30.0):
        print("classC",end=" ")
    else:
        print("classD(Your BMI is",b,")")
    
