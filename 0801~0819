print(v[0],end=" ")
print(v[1],end=" ")
print(v[2],end=" ")
print(v[3],end=" ")

v=[0 for x in range(4)]
for x in range(len(v)):
    print(v[0],end=" ")

v=[0 for x in range(10)]
for x in range(len(v)):
    print(v[0],end=" ")

