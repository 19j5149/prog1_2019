a=66
b=88
r=(a+b)/2
print("average is",r)

def ave_2():
    a=66
    b=88
    r=(a+b)/2
    print("average is",r)
ave_2()

def ave_2():
    a=66
    b=88
    r=(a+b)/2
    return (r)
r=ave_2()
print("average is",r)

def ave_2(a,b):
    r=(a+b)/2
    return (r)
r=ave_2(66,88)
print("average is",r)

def ave_4(a,b,c,d):
    r=(a+b+c+d)/4
    return (r)
r=ave_4(1,2,3,4)
print("average is",r)

a=[1,2,3,4]
print(a[0],end=" ")
print(a[1],end=" ")
print(a[2],end=" ")
print(a[3],end=" ")

a=[1,2,3,4]
for x in range(4):
    print(x+1,end=" ")
   
a=[1,2,3,4]
for x in range(len(a)):
    print(x+1,end=" ")

a=[1,2,3,4]
s=0
for x in range(len(a)):
    s=s+a[x]
r=s/len(a)
print("average is",r)

def ave_n(a):
    s=0
    for x in range(len(a)):
        s=s+a[x]
    r=s/len(a)
    return r
a=[1,2,3,4,5,6,7,8]
r=ave_n(a)
print("average is",r)

